{
	"manifest_version": 2,

	"name": "Research Tree",
	"description": "Tab workflow tracker",
	"version": "1.0",

	"icons": {
		"16": "images/icon-16.png"
	},

	"browser_action": {
		"default_icon": "images/icon2.png"
	},

	"background": {
		"scripts": ["background.js"]
	},

	"permissions": [
		"tabs",
		"contextMenus"
	]
}
